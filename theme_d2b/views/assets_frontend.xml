<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <template id="theme_d2b_assets_fronted" 
        name="Theme D2B Assets Frontend" 
        inherit_id="website.assets_frontend">

        <xpath expr="//script[last()]" position="after">
            <link rel="stylesheet" type="text/css" 
                href="/theme_d2b/static/src/assets/css/main.css"/>
            <link rel="stylesheet" type="text/css" 
                href="/theme_d2b/static/src/assets/css/noscript.css"/>
            <link rel="stylesheet" type="text/css"
                href="/theme_d2b/static/src/assets/css/fontawesome-all.min.css"/>
        </xpath>

    </template>

</odoo>